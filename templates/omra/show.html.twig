{% block title %}Omra{% endblock %}

{% block body %}
    <h1>Omra</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ omra.id }}</td>
            </tr>
            <tr>
                <th>Date_debut</th>
                <td>{{ omra.dateDebut ? omra.dateDebut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Date_fin</th>
